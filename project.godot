; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="STS x JRPG"
run/main_scene="res://Scenes/gameplay/combat_scene.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://Graphics/ui/cursor_MAYBE.png"

[autoload]

Globals="*res://Utilities/Globals/Globals.gd"
CombatManager="*res://Utilities/Globals/CombatManager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=640
window/size/window_height_override=360
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Databases/": "gray",
"res://Graphics/": "red",
"res://Resources/": "teal",
"res://Scenes/": "purple",
"res://Scripts/": "green",
"res://Utilities/": "orange"
}

[global_group]

in_party="character in the current party"
character="recruitable party members & player"
mob="regular enemies"
elite="elite enemies"
boss="boss enemies"

[gui]

theme/custom_font="res://Utilities/Fonts/Abaddon Bold.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
