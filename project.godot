config_version=5

[application]

config/name="STS x JRPG"
run/main_scene="res://Scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://Graphics/ui/cursor_MAYBE.png"

[autoload]

InventoryManager="*res://Utilities/Globals/InventoryManager.gd"
Events="*res://Utilities/Globals/events.gd"
SessionManager="*res://Utilities/Globals/SessionManager.gd"
GlobalParty="*res://Resources/Party/GlobalParty.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=920
window/size/window_height_override=520
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Databases/": "gray",
"res://Graphics/": "red",
"res://Resources/": "teal",
"res://Scenes/": "purple",
"res://Scripts/": "green",
"res://Utilities/": "orange"
}

